<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.aquafinaapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_payment"
        android:name="com.example.aquafinaapp.ui.payment.PaymentFragment"
        android:label="@string/title_payment"
        tools:layout="@layout/fragment_payment" />

    <fragment
        android:id="@+id/navigation_userInfo"
        android:name="com.example.aquafinaapp.ui.userInfo.userInfoFragment"
        android:label="@string/title_userInfo"
        tools:layout="@layout/fragment_user_info" />

    <fragment
        android:id="@+id/navigation_faq"
        android:name="com.example.aquafinaapp.ui.faq.FAQFragment"
        android:label="@string/title_faq"
        tools:layout="@layout/fragment_user_info" />

</navigation>