<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aquafinaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AquaFinaApp">
        <activity
            android:name=".ui.userInfo.aboutUs"
            android:exported="false" />
        <activity
            android:name=".ui.userInfo.updateUserInfo"
            android:exported="false" />
        <activity
            android:name=".ui.userInfo.viewUserInfo"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.paymentSuccessful.paymentSuccessful"
            android:exported="false" />
        <activity
            android:name=".ui.loginPage.loginPage"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>